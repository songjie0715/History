{"version":3,"file":"../activity.css","sources":["activity.scss"],"sourcesContent":["$font-static: \"PingHei\",Helvetica,Arial,Verdana,sans-serif;\n$primary-tit-color: #757575;\n\n@function size($n) {\n  @return $n / 20;\n}\n\n*{\n  margin: 0;\n  padding: 0;\n}\nhtml,body{\n  height: 100%;\n}\nbody{\n  overflow-x: hidden;\n}\nul,ol,li{\n  list-style: none;\n}\n\n.activity-free{\n  padding-bottom: 30px;\n  .bd{\n    padding:0 size(18rem);\n  }\n  .book-module{\n    position: relative;\n    padding: size(12rem) 0 size(20rem) 0;\n    border-top: 0.5px solid #d8dacf;\n    &:first-child{\n      border-top: none;\n    }\n    h2{\n      position: relative;\n      font: bold 15px $font-static;\n      color: $primary-tit-color;\n      padding-bottom: size(8rem);\n      padding-left: 18px;\n      .icon-tit{\n        display: block;\n        position: absolute;\n        border: none;\n        left: 0;\n        top: 3px;\n        width: 12.5px;\n        height: 15px;\n        background: url(../img/bg-tit.png) 0 0 no-repeat;\n        -webkit-background-size: 50px auto;\n        background-size: 50px auto;\n      }\n    }\n    ul{\n      display: inline-block;\n      margin-left: -size(19rem);\n    }\n    li{\n      position: relative;\n      float: left;\n      width: size(100rem);\n      margin-left: size(19rem);\n      overflow: hidden;\n      &:first-child{\n        margin-left: 0;\n      }\n      a{\n        display: block;\n        text-decoration: none;\n      }\n      img{\n        width: 100%;\n      }\n      dd{\n        padding-top: size(5rem);\n        font-size: size(14rem);\n        line-height: size(18rem);\n        color: $primary-tit-color;\n      }\n      .icon-free{\n        position: absolute;\n        top: 5px;\n        right: -14px;\n        width: size(56rem);\n        height: size(12rem);\n        line-height: size(12rem);\n        background: #3399ff;\n        -webkit-transform: rotate(38deg);\n        transform: rotate(38deg);\n        text-align: center;\n        font-size: 12px;\n        color: #fff;\n        font-style: normal;\n      }\n    }\n    &.female h2 .icon-tit{\n      background-position: 0 -20px;\n    }\n    &.publish h2 .icon-tit{\n      background-position: 0 -40px;\n    }\n  }\n  .activity-detail{\n    border-top: 0.5px solid #d8dacf;\n    padding: size(14rem) size(18rem) 0;\n    h3{\n      padding-bottom: size(8rem);\n      font: normal size(15rem) $font-static;\n      color: #444;\n    }\n    p{\n      color: #777;\n      font-size: size(14rem);\n      line-height: size(22rem);\n      span{\n        font-family: Arial;\n      }\n    }\n  }\n  .count-down{\n    position: relative;\n    width: 100%;\n    padding: size(20rem) 0;\n    font: normal 15px $font-static;\n    display: inline-block;\n    border-bottom: 1px solid #d8dacf;\n    .bd{\n      text-align: center;\n      position: relative;\n      left: 50%;\n      width: 100%;\n      transform: translateX(-50%);\n    }\n    .well{\n      display: inline-block;\n    }\n  }\n}\n\n\n\n.countdown-clock {\n  display: inline-block;\n  margin-left: 4px;\n  font-size: 15px;\n  text-align: center;\n}\n\n.countdown-clock .years,\n.countdown-clock .months,\n.countdown-clock .days,\n.countdown-clock .hours,\n.countdown-clock .minutes,\n.countdown-clock .seconds {\n  font-size: 15px;\n  display: inline-block;\n  background-color:#45454d;\n  color:#fff;\n  border-radius:5px;\n  width: 31px;\n  height: 20px;\n  background: #fff url(../img/bg-tit.png) 0 -60px no-repeat;\n  -webkit-background-size: 50px auto;\n  background-size: 50px auto;\n  vertical-align: 1px;\n}\n.countdown-clock span:first-of-type {\n  margin-left: 0;\n  width: 15px;\n}\n.countdown-clock span i{\n  display: inline-block;\n  padding: 0 3.5px;\n  font-style: normal;\n  text-align: left;\n}\n\n.radius{\n  width: 100px;\n  height: 100px;\n  border: 1px solid #000;\n  background: #ccc;\n  -webkit-border-radius: 50%;\n  -moz-border-radius: 50%;\n  border-radius: 50%;\n  transform: rotate(0deg);\n  transition: all 5s ease;\n\n}"],"mappings":"AAOA,AAAA,CAAC,CAAA;EACC,MAAM,EAAE,CAAE;EACV,OAAO,EAAE,CAAE,GACZ;;AACD,AAAA,IAAI,EAAC,AAAA,IAAI,CAAA;EACP,MAAM,EAAE,IAAK,GACd;;AACD,AAAA,IAAI,CAAA;EACF,UAAU,EAAE,MAAO,GACpB;;AACD,AAAA,EAAE,EAAC,AAAA,EAAE,EAAC,AAAA,EAAE,CAAA;EACN,UAAU,EAAE,IAAK,GAClB;;AAED,AAAA,cAAc,CAAA;EACZ,cAAc,EAAE,IAAK,GAkHtB;EAnHD,AAEE,cAFY,CAEZ,GAAG,CAAA;IACD,OAAO,EAAC,CAAC,CApBH,MAAE,GAqBT;EAJH,AAKE,cALY,CAKZ,YAAY,CAAA;IACV,QAAQ,EAAE,QAAS;IACnB,OAAO,EAxBD,MAAE,CAwBa,CAAC,CAxBhB,IAAE,CAwB2B,CAAC;IACpC,UAAU,EAAE,mBAAoB,GAuEjC;IA/EH,AAKE,cALY,CAKZ,YAAY,AAIT,YAAY,CAAA;MACX,UAAU,EAAE,IAAK,GAClB;IAXL,AAYI,cAZU,CAKZ,YAAY,CAOV,EAAE,CAAA;MACA,QAAQ,EAAE,QAAS;MACnB,IAAI,EAAE,IAAI,CAAC,IAAI,CAnCP,SAAS,EAAC,SAAS,EAAC,KAAK,EAAC,OAAO,EAAC,UAAU;MAoCpD,KAAK,EAnCS,OAAO;MAoCrB,cAAc,EAjCV,MAAE;MAkCN,YAAY,EAAE,IAAK,GAapB;MA9BL,AAkBM,cAlBQ,CAKZ,YAAY,CAOV,EAAE,CAMA,SAAS,CAAA;QACP,OAAO,EAAE,KAAM;QACf,QAAQ,EAAE,QAAS;QACnB,MAAM,EAAE,IAAK;QACb,IAAI,EAAE,CAAE;QACR,GAAG,EAAE,GAAI;QACT,KAAK,EAAE,MAAO;QACd,MAAM,EAAE,IAAK;QACb,UAAU,EAAuB,sBAAC,CAAC,CAAC,CAAC,CAAC,CAAC,SAAS;QAChD,uBAAuB,EAAE,SAAU;QACnC,eAAe,EAAE,SAAU,GAC5B;IA7BP,AA+BI,cA/BU,CAKZ,YAAY,CA0BV,EAAE,CAAA;MACA,OAAO,EAAE,YAAa;MACtB,WAAW,EAAE,YAAK,GACnB;IAlCL,AAmCI,cAnCU,CAKZ,YAAY,CA8BV,EAAE,CAAA;MACA,QAAQ,EAAE,QAAS;MACnB,KAAK,EAAE,IAAK;MACZ,KAAK,EAvDD,IAAE;MAwDN,WAAW,EAxDP,OAAE;MAyDN,QAAQ,EAAE,MAAO,GAgClB;MAxEL,AAmCI,cAnCU,CAKZ,YAAY,CA8BV,EAAE,AAMC,YAAY,CAAA;QACX,WAAW,EAAE,CAAE,GAChB;MA3CP,AA4CM,cA5CQ,CAKZ,YAAY,CA8BV,EAAE,CASA,CAAC,CAAA;QACC,OAAO,EAAE,KAAM;QACf,eAAe,EAAE,IAAK,GACvB;MA/CP,AAgDM,cAhDQ,CAKZ,YAAY,CA8BV,EAAE,CAaA,GAAG,CAAA;QACD,KAAK,EAAE,IAAK,GACb;MAlDP,AAmDM,cAnDQ,CAKZ,YAAY,CA8BV,EAAE,CAgBA,EAAE,CAAA;QACA,WAAW,EArET,OAAE;QAsEJ,SAAS,EAtEP,MAAE;QAuEJ,WAAW,EAvET,MAAE;QAwEJ,KAAK,EA3EO,OAAO,GA4EpB;MAxDP,AAyDM,cAzDQ,CAKZ,YAAY,CA8BV,EAAE,CAsBA,UAAU,CAAA;QACR,QAAQ,EAAE,QAAS;QACnB,GAAG,EAAE,GAAI;QACT,KAAK,EAAE,KAAM;QACb,KAAK,EA9EH,MAAE;QA+EJ,MAAM,EA/EJ,MAAE;QAgFJ,WAAW,EAhFT,MAAE;QAiFJ,UAAU,EAAE,OAAQ;QACpB,iBAAiB,EA