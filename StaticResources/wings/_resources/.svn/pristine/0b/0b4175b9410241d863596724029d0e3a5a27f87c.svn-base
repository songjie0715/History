define(['core'], function(core){
    return {
        getTickets: function(page){
            return $.ajax({
                url: core.website_DOMAIN + '/ajax/i/ticket',
                data: {
                    page: page
                }
            })
        },
        getPackageTickets: function(page){
            return $.ajax({
                url: core.website_DOMAIN + '/ajax/i/ticket',
                data: {
                    page: page
                }
            })
        }
    }
});