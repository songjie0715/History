body,html{width:100%;height:100%}body{background:#fff}.site_box .site_list1{margin:.15rem;padding-bottom:.15rem;border-bottom:1px solid #ddd}.site_box .site_list1:nth-child(last){border:0}.site_box .site_list1 .site_name{display:block;width:90%;margin-left:5%;height:1.4rem;border:0;overflow:hidden;resize:none;line-height:.18rem;font-size:.14rem;color:#000;margin-top:.18rem}.tishi{padding:.15rem}.tishi .wenxin{height:.24rem}.tishi .wenxin img{display:block;width:.9rem;height:.26rem}.tishi .wenxin_text{margin-top:.1rem;line-height:.2rem}.tishi .wenxin_text p:nth-child(1){font-size:.14rem;color:#000;font-weight:700}.tishi .wenxin_text p:nth-child(2){font-size:.14rem;color:#f66;font-weight:700}