idth:100%}.banner-mod .banBd li a{position:relative;display:block;width:100%}.banner-mod .banBd li a span{position:absolute;bottom:0;left:0;display:block;padding-left:1.5rem;width:100%;height:2.5rem;font-size:1.4rem;line-height:2.5rem;background:#333;opacity:.7;color:#fff}.public-mod{margin-bottom:1rem;background:#fff;overflow:hidden}.public-mod h3{position:relative;padding:0 1.5rem;line-height:3rem;font-size:1.4rem;border-bottom:solid 1px #e6e6e6;color:#666;font-weight:700}.public-mod h3 a{position:absolute;top:0;right:1.5rem;display:block;font-size:1.4rem;color:#999;font-weight:400}.public-mod .public-mod-list{overflow:hidden}.public-mod .public-mod-list a{padding-left:1.5rem;background:#fff}.public-mod a{display:block}.public-mod .list-dl-mod{display:-webkit-box;display:-moz-box;display:box;padding:1rem 0;overflow:hidden}.public-mod .list-dl-mod dt{margin-left:15px}.public-mod .list-dl-mod dd{-webkit-box-flex:1;-moz-box-flex:1;box-flex:1;font-size:1.2rem;overflow:hidden}.public-mod .list-dl-mod dd .title{display:block}.public-mod .list-dl-mod dd .title a{color:#333;font-size:1.6rem}.public-mod .list-dl-mod dd .title a i{display:block;float:right;font-weight:400;color:#ff4c2f;font-size:1.2rem}.public-mod .list-dl-mod dd .tag{padding:.3rem 0 .4rem;display:block;color:#999}.public-mod .list-dl-mod dd .update a{display:block;color:#333;line-height:1.8rem}.public-mod .list-dl-mod dd .update span{display:block;white-space:nowrap;overflow:hidden;text-overflow:ellipsis}.public-mod .list-dl-mod dd .text{margin-bottom:1.5rem;font-size:1.5rem;color:#333;height:4.2rem;line-height:2.1rem;overflow:hidden}.public-mod .list-dl-mod dd .authorImg{display:block;color:#ccc}.public-mod .list-dl-mod dd .authorImg img{margin-right:.5rem;width:1rem;vertical-align:-2px}.public-mod .first-dl-mod{padding:1.5rem 1.5rem 1rem}.public-mod .first-dl-mod dt{margin-right:15px;margin-left:0}.public-mod .first-dl-mod dt img{width:5rem;display:block;border-radius:5px;box-shadow:0 0 2px #ccc}.public-