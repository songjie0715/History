{"version":3,"file":"../bookDetail.css","sources":["bookDetail.scss","_globalVar.scss","dialog.scss"],"sourcesContent":["@import \"globalVar\";\n@import \"dialog\";\n\nhtml{\n  font-size: 62.5%;\n  -webkit-text-size-adjust: 100%;\n}\nbody{\n  width: 100%;\n  //max-width: 640px;\n  font-family: \"PingFang SC\",\"Microsoft YaHei\", \"微软雅黑\";\n}\n*{\n  padding: 0;\n  margin: 0;\n}\n.fl{\n  float: left;\n}\n.fr{\n  float: right;\n}\n.ovh{\n  overflow: hidden;\n}\n\n.hidden{\n  display: none!important;\n}\n\n.text-center{\n  text-align: center;\n}\n\na{\n  color: #666;\n  text-decoration: none;\n}\nul,li{\n  list-style: none;\n}\ni{\n  font-style: normal;\n}\n\n[v-cloak]{\n  display: none;\n}\n\n.textOverFlow{\n  overflow: hidden;\n  -ms-text-overflow: ellipsis;\n  text-overflow: ellipsis;\n  white-space: nowrap;\n}\n\n\n\n\n\n\n\nhtml,body{\n  min-height: 100%;\n}\n\n.wrapper{\n  position: relative;\n  min-height: 100%;\n  background: #fff;\n  margin: 0 auto;\n  padding-bottom: 60px;\n  width: 100%;\n  font-size: 12px;\n  .book-detail{\n    width: 100%;\n    margin-top: 0;\n    background: #fff;\n    &:first-child{\n      margin-top: 0!important;\n    }\n    >.bd{\n      padding: 1.2rem 0 0 0;\n    }\n    // 头像\n    .pic{\n      overflow: hidden;\n      width: 32px;\n      height: 32px;\n      border-radius: 32px;\n      img{\n        border-radius: 32px;\n      }\n    }\n    // 更多\n    .more{\n      border-top: 1px solid $text-line;\n      margin: 0 -1.2rem;\n      margin-bottom: -1rem;\n      margin-top: -1rem;\n      text-align: center;\n      height: 3.8rem;\n      line-height: 3.8rem;\n      font-size: 12px;\n    }\n    // 按钮\n    .btn{\n      display: block;\n      height: 2.2rem;\n      line-height: 2.2rem;\n      padding: 0 0.8rem;\n      border: 1px solid #22bbee;\n      border-radius: 0.6rem;\n      color: #22bbee;\n      font-size: 12px;\n    }\n\n    // 箭头\n    .arrow{\n      border: 0.5rem solid #999;\n      i{\n        position: absolute;\n        border: 0.5rem solid #fff;\n        top: -8px;\n        left: -8px;\n      }\n    }\n    .arrow-right{\n      display: inline-block;\n      width: 6px;\n      height: 12px;\n      background: url(\"../../img/wx/laikan/bg-book-detail.png\") 0 -15px no-repeat;\n      -webkit-background-size: auto 60px;\n      background-size: auto 60px;\n      vertical-align: -2px;\n      margin-left: 7px;\n    }\n\n    .arrow-left{\n      border-left: 0.5rem solid transparent;\n      border-top: 0.5rem solid transparent;\n      transform: rotate(135deg);\n      i{\n        position: absolute;\n        top: -8px;\n        left: -8px;\n        border: 0.5rem solid #fff;\n        border-left: 0.5rem solid transparent;\n        border-top: 0.5rem solid transparent;\n      }\n    }\n\n    // 评论列表\n    .book-set .comment-lists{\n      margin-top: 10px;\n      overflow: hidden;\n\n      .list-item{\n        padding: 15px 0;\n        border-top: 1px solid #f5f5f5;\n        .item{\n          overflow: hidden;\n          padding-left: 20px;\n          font-size: 12px;\n        }\n        h3{\n          line-height: 1.5rem;\n          color: #66ccdd;\n        }\n        h4{\n          line-height: 1.5rem;\n        }\n        p{\n          margin-top: 6px;\n          line-height: 1.4rem;\n          color: $text-content;\n          word-break: break-all;\n          &.upfold{\n            height: 4rem;\n          }\n        }\n        .date{\n          float: right;\n          line-height: 2rem;\n          color: $text-msg;\n        }\n        .item-handle{\n          float: right;\n          overflow: hidden;\n          margin-top: 1rem;\n          li{\n            float: left;\n            width: 6rem;\n            text-align: right;\n            span.icon{\n              display: inline-block;\n              margin-right: 6px;\n              width: 16px;\n              height: 15px;\n              vertical-align: -2px;\n              background: url(\"../../img/wx/laikan/bg-book-detail.png\") -20px 0 no-repeat;\n              -webkit-background-size: auto 60px;\n              background-size: auto 60px;\n            }\n            i{\n              display: inline-block;\n              padding-top: 0.2rem;\n            }\n            &.like span.icon{\n              \n            }\n            &.like.do span.icon{\n              background-position: -38px 0;\n            }\n            &.comment span.icon{\n              width: 15px;\n              height: 14px;\n              background-position: -58px 0;\n            }\n          }\n          a{\n            color: $text-content;\n          }\n        }\n      }\n    }\n    .noComment{\n      padding: 20px 0;\n      text-align: center;\n      font-size: 14px;\n      border-top: 1px solid $text-line;\n      a{\n        height: 24px;\n        color: $text-content;\n        text-decoration: none;\n        span{\n          display: block;\n          margin: 0 auto 10px;\n          width: 35px;\n          height: 32px;\n          background: url(\"../../img/wx/laikan/bg-book-detail.png\") 0 -30px no-repeat;\n          -webkit-background-size: auto 60px;\n          background-size: auto 60px;\n        }\n      }\n    }\n\n\n\n    // 三角箭头\n\n    .upfold-more{\n      position: absolute;\n      width: 3rem;\n      height: 3rem;\n      right: 0;\n      text-align: center;\n      .arrow{\n        display: inline-block;\n        margin-top: 17px;\n        border: 0.5rem solid #999;\n        border-bottom: 0 solid transparent;\n        border-left: 0.5rem solid transparent;\n        border-right: 0.5rem solid transparent;\n        -webkit-transition: transform 0.5s;\n        transition: transform 0.5s;\n      }\n      .arrowNew{\n        display: block;\n        margin-top: 10px;\n        width: 20px;\n        height: 10px;\n        background: url(\"../../img/wx/laikan/bg-book-detail.png\") 0 0 no-repeat;\n        -webkit-background-size: auto 60px;\n        background-size: auto 60px;\n        -webkit-transition: transform 0.5s;\n        transition: transform 0.5s;\n      }\n      .arrow-down{\n        -webkit-transform: rotate(0deg);\n        transform: rotate(0deg);\n      }\n      .arrow-up{\n        -webkit-transform: rotate(180deg);\n        transform: rotate(180deg);\n      }\n    }\n\n    .total{\n      height: 48px;\n      line-height: 48px;\n      text-align: center;\n      color: #66ccdd;\n      font-size: 14px;\n      border-top: 1px solid #f5f5f5;\n      >a{\n        padding: 0!important;\n        color: #66ccdd!important;\n      }\n      i{\n        display: inline-block;\n        width: 6px;\n        height: 12px;\n        background: url(\"../../img/wx/laikan/bg-book-detail.png\") 0 -15px no-repeat;\n        -webkit-background-size: auto 60px;\n        background-size: auto 60px;\n        vertical-align: -2px;\n        margin-left: 7px;\n      }\n    }\n\n\n    .book-comment{\n      position: relative;\n      .reply-comment{\n        position: absolute;\n        right: 0;\n        top: -27px;\n        color: #66ccdd;\n        span{\n          display: inline-block;\n          width: 12px;\n          height: 13px;\n          background: url(\"../../img/wx/laikan/bg-book-detail.png\") -10px -15px no-repeat;\n          -webkit-background-size: auto 60px;\n          background-size: auto 60px;\n          vertical-align: -2px;\n        }\n      }\n    }\n    .book-info-wrapper{\n      margin-top: -20px;\n      padding: 25px 15px 12px;\n      background: #f2f2f2;\n      border-bottom: 1px solid $text-line;\n    }\n    .book-info{\n      display: inline-block;\n      width: 100%;\n      position: relative;\n      margin-top: 6px;\n      .add-to-shelf{\n        z-index: 10;\n        position: absolute;\n        padding-left: 10px;\n        right: -15px;\n        top: 70px;\n        width: 62px;\n        height: 20px;\n        line-height: 20px;\n        background: #66ccdd;\n        font-size: 1.2rem;\n        color: #fff;\n        border-radius: 5px 0 0 5px;\n        &.done{\n          background: #ccc;\n          i{\n            display: none;\n          }\n        }\n      }\n      .book-pic{\n        position: relative;\n        overflow: hidden;\n        width: 80px;\n        -webkit-box-shadow: 1px 1px 1px #cacaca;\n        box-shadow: 1px 1px 1px #cacaca;\n        @include border-radius(5px);\n        img{\n          @include border-radius(5px);\n        }\n        .discount, .free, .whole-free{\n          position: absolute;\n          right: 6px;\n          top: -2px;\n          padding-top: 9px;\n          width: 28px;\n          height: 28px;\n          text-align: center;\n          font-size: 12px;\n          background: #f18e2c;\n          opacity: .9;\n          color: #fff;\n          -webkit-transform: scale(1);\n          transform: scale(1);\n          &::after{\n            content: '';\n            display: inline-block;\n            border: 6px solid #f18e2c;\n            border-top: none;\n            border-left: 10px solid transparent!important;\n            border-right: 10px solid transparent!important;\n            -webkit-transform: rotate(180deg) scale(1.5) translateY(3px);\n            transform: rotate(180deg) scale(1.5) translateY(3px);\n          }\n        }\n        .free{\n          background-color: #66ccdd;\n          &::after{\n            border-color: #66ccdd;\n          }\n        }\n        .whole-free{\n          background-color: #ff4c2f;\n          &::after{\n            border-color: #ff4c2f;\n          }\n        }\n        img{\n          display: block;\n        }\n      }\n      .info{\n        overflow: hidden;\n        padding-left: 1.2rem;\n        color: #333;\n        h2{\n          margin: 10px 0 8px 0;\n          font-size: 1.6rem;\n        }\n        h3{\n          a,span{\n            color: $text-content;\n          }\n          font-size: 1.2rem;\n          line-height: 18px;\n        }\n      }\n      .tip{\n        font-size: 12px;\n        color: $text-content;\n        i.spec{\n          color: $text-imp;\n        }\n      }\n    }\n\n\n    .book-content{\n      position: relative;\n      padding: 12px 0;\n      font-size: 1.2rem;\n      line-height: 1.4rem;\n      color: $text-content;\n      border-bottom: 2px solid $text-line;\n      text-align: center;\n      p{\n        text-align: left;\n        line-height: 18px;\n        padding: 0 15px;\n        &.upfold{\n          height: 56px;\n          overflow: hidden;\n        }\n      }\n      .tag{\n        //position: absolute;\n        overflow: hidden;\n        margin-top: 0.3rem;\n        margin-left: 0;\n        li{\n          float: left;\n          margin: 0.4rem 0.2rem;\n          padding: 0.2rem 0.4rem;\n          text-align: center;\n          border-radius: 5px;\n          font-size: 12px;\n          a{\n            color: #fff;\n          }\n          &.yel{\n            background: #eb9e43;\n          }\n          &.blue{\n            background: #72a0ff;\n          }\n          &.pink{\n            background: #ff72a4;\n          }\n        }\n        &.upfold{\n          overflow: hidden;\n          height: 28px;\n          padding-right: 60px;\n        }\n      }\n      .upfold-more{\n        display: inline-block;\n        position: relative;\n        height: 1.5rem;\n        text-align: center;\n        border: 1px solid #fff;\n        .arrow{\n          margin-top: 1rem;\n        }\n      }\n      .arrow{\n        display: inline-block;\n        margin-top: 17px;\n        border: 7px solid #999;\n        border-bottom: 0 solid transparent;\n        border-left: 7px solid transparent;\n        border-right: 7px solid transparent;\n        -webkit-transition: all 0.3s ease-in;\n        transition: all 0.3s ease-in;\n      }\n      .arrow-down{\n        -webkit-transform: rotate(0deg);\n        transform: rotate(0deg);\n      }\n      .arrow-up{\n        -webkit-transform: rotate(180deg);\n        transform: rotate(180deg);\n      }\n    }\n    .book-lists{\n      position: relative;\n      font-size: 14px;\n      color: #333;\n      display: block;\n      a{\n        display: block;\n        padding: 12px 0;\n        color: #333;\n      }\n      span{\n        display: block;\n        font-size: 12px;\n        color: #666;\n        &.serialize{\n          overflow: hidden;\n          width: 82%;\n          font-size: 13px;\n          i{\n            display: inline-block;\n            margin-top: 5px;\n            font-size: 12px;\n            color: $text-msg;\n          }\n        }\n      }\n      .time{\n        display: inline-block;\n        float: left;\n        width: 50px;\n        font-size: 13px;\n        color: #66ccdd;\n        i{\n          font-family: Arial;\n        }\n      }\n      .arrow-right{\n        position: relative;\n        display: block;\n        border: 6px solid #999;\n        border-left: 6px solid transparent;\n        border-top: 6px solid transparent;\n        width: 0;\n        height:0;\n        padding:0;\n        -webkit-transition: all .5s ease-in;\n        transition: all .5s ease-in;\n        -webkit-transform: rotate(-45deg);\n        transform: rotate(-45deg);\n        i{\n          position: absolute;\n          top: -7px;\n          left: -7px;\n          border: 6px solid #fff;\n          border-left: 6px solid transparent;\n          border-top: 6px solid transparent;\n        }\n      }\n      .status{\n        position: absolute;\n        right: 0;\n        top: -17px;\n        font-size: 12px;\n        color: #00d49b;\n      }\n    }\n    .book-handle{\n      .tab{\n        overflow: hidden;\n        li{\n          float: left;\n          font-size: 1.4rem;\n          color: #333;\n          padding-bottom: 0.4rem;\n          &.active{\n            color: #22bbee;\n            border-bottom: 1px solid #22bbee;\n          }\n          &.gap{\n            color: #666;\n            border-left: 1px solid #999;\n            margin: 5px 1rem 0;\n            height: 0.9rem;\n          }\n        }\n      }\n      .book-container{\n\n        .price-list{\n          margin: 14px 0;\n          overflow: hidden;\n          li{\n            float: left;\n            width: 20%;\n            text-align: center;\n            span{\n              display: inline-block;\n              width: 4.5rem;\n              height: 4.5rem;\n              background: #22bbee;\n              color: #fff;\n              font-size: 12px;\n              border-radius: 4.5rem;\n              text-align: center;\n              b{\n                display: block;\n                margin-top: 1rem;\n                line-height: 12px;\n              }\n              b,i{\n                font-size: 12px;\n              }\n            }\n            &:nth-child(1) span{\n              background: #ffdc2e;\n            }\n            &:nth-child(2) span{\n              background: #ffb92e;\n            }\n            &:nth-child(3) span{\n              background: #ff962e;\n            }\n            &:nth-child(4) span{\n              background: #ff742e;\n            }\n            &:nth-child(5) span{\n              background: #ff4c2f;\n            }\n          }\n        }\n        .reward-list{\n          margin-top: 16px;\n          height: 130px;\n          padding: 15px 4px;\n          overflow: hidden;\n          &.rewrad-list-init{\n            height: auto;\n            margin-bottom: -28px;\n          }\n          li{\n            height: 44px;\n            &.first{\n              transition: all 1.5s;\n              transform: translateY(-50px);\n              opacity: 0;\n              height: 45px;\n              position: absolute;\n            }\n            &.trans{\n              height: 45px;\n              transform: translateY(0px);\n              opacity: 1;\n              position: relative;\n            }\n            &.last{\n              transition: all 1.5s;\n              transform: translateY(50px);\n            }\n          }\n          .reward-info{\n            display: inline-block;\n          }\n          .pic{\n            margin-top: -10px;\n            margin-right: 0.8rem;\n          }\n          .fool{\n            font-family: Arial;\n          }\n          .voucher{\n            color: #22bbee;\n            i{\n              font-family: Arial;\n            }\n            b{\n              font-weight:normal;\n            }\n          }\n          .date{\n            color: $text-msg;\n            font-family: Arial;\n          }\n          .action{\n            margin: -5px 4px 0;\n            padding: 0 4px;\n            width: 24px;\n            height: 24px;\n            background: url('../images/icon-reward.gif#{$version}') center center no-repeat;\n            -webkit-background-size: 24px 24px;\n            background-size: 24px 24px;\n            font-size: 0;\n          }\n        }\n        .book-enjoy .more{\n          margin-top: 8px;\n        }\n      }\n    }\n    .book-module{\n      position: relative;\n      padding: 0 15px;\n      .tit{\n        color: $text-txt;\n        font-size: 1.4rem;\n      }\n      .change-items{\n        position: absolute;\n        right: 15px;\n        top: 0.2rem;\n        color: #66ccdd;\n        font-size: 12px;\n      }\n      .book-items{\n        margin-top: 1.1rem;\n        h3{\n          font-size: 1.5rem;\n          color: $text-txt;\n          line-height: 1.5rem;\n        }\n        h4{\n          padding-top: 4px;\n          font-size: 1.3rem;\n          line-height: 1.5rem;\n          color: $text-txt;\n        }\n        p{\n          padding-top: 0.8rem;\n          color: $text-msg;\n          height: 5.4rem;\n          line-height: 1.5rem;\n          font-size: 12px;\n        }\n        dt{\n          -webkit-box-shadow: 1px 1px 1px #cacaca;\n          box-shadow: 1px 1px 1px #cacaca;\n          img{\n            display: block;\n          }\n        }\n      }\n      .layout-l-r{\n        a{\n          display: block;\n        }\n        dl{\n          display: inline-block;\n          padding: 0.7rem 0;\n          dt{\n            float: left;\n          }\n          dd{\n            overflow: hidden;\n            padding: 0.6rem 0 0 1.4rem;\n            p{\n              word-break: break-all;\n            }\n          }\n        }\n      }\n      .layout-t-b{\n        overflow: hidden;\n        li{\n          float: left;\n          width: 33.33%;\n          text-align: center;\n          border: 0;\n          padding: 0;\n          a{\n            display: inline-block;\n            width: 80px;\n          }\n          h3{\n            padding-top: 0.5rem;\n            width: 7rem;\n            overflow: hidden;\n            text-overflow: ellipsis;\n            white-space: nowrap;\n            text-align: left;\n            font-size: 12px;\n          }\n        }\n      }\n      .more{\n        margin-top: 0.7rem;\n      }\n    }\n  }\n  // 书架\n  .book-shelf{\n    .book-module{\n      .book-items{\n        margin-top: 0;\n        border-bottom: 1px solid #ddd;\n        li{\n          position: relative;\n          padding: 5px 0;\n          border-top: 1px solid #ddd;\n          &:first-child{\n            border-top: none;\n          }\n        }\n        h4{\n          padding-top: 6px;\n          line-height: 14px;\n          a{\n            color: $text-msg;\n          }\n        }\n        dl dd{\n          padding-top: 10px;\n          a{\n            display: inline-block;\n          }\n        }\n        p{\n          color: $text-content;\n          font-size: 13px;\n        }\n        .goon{\n          position: absolute;\n          top: 50%;\n          right: 0;\n          margin-top: -8px;\n          color: $text-imp;\n          font-size: 1.4rem;\n        }\n        .del{\n          position: absolute;\n          top: 50%;\n          right: 0;\n          margin-top: -8px;\n          color: $text-imp;\n          border-color: #ee5048;\n        }\n      }\n      .have-no-books{\n        padding: 50px 0;\n        text-align: center;\n        color: $text-msg;\n        font-size: 14px;\n        a{\n          position: relative;\n          display: inline-block;\n          width: 37.5%;\n          height: 80px;\n          border: 1px dashed #ccc;\n          border-radius: 4px;\n          text-align: center;\n          span{\n            margin: 0 auto;\n            display: block;\n            width: 3.2rem;\n            height: 0.4rem;\n            background: #ccc;\n            -webkit-transition: all;\n            transition: all;\n            &.hor{\n              position: absolute;\n              top: 50%;\n              left: 50%;\n              margin-top: -0.2rem;\n              margin-left: -1.6rem;\n\n            }\n            &.ver{\n              -webkit-transform: rotate(90deg);\n              transform: rotate(90deg);\n            }\n          }\n        }\n        p{\n          padding: 8px 0;\n        }\n      }\n    }\n\n    &.book-shelf-add{\n      .shelf-tab{\n        ul{\n          display: inline-block;\n          width: 100%;\n        }\n        li{\n          float: left;\n          width: 49.5%;\n          height: 40px;\n          line-height: 40px;\n          font-size: 16px;\n          text-align: center;\n          color: $text-txt;\n          border-bottom: 1px solid #ddd;\n          &.active{\n            border-color: #22bbee;\n          }\n          &.gap{\n            margin-top: 12px;\n            border-left: 1px solid #ddd;\n            height: 10px;\n            width: 0.5px;\n          }\n        }\n      }\n    }\n  }\n  // 其他\n  .book-others{\n    .book-module .book-items{\n      margin-top: 0;\n      li{\n        border-top: 1px solid #ddd;\n        padding: 5px 0;\n        &:first-child{\n          border:none;\n        }\n        dl dd{\n          margin-top: 10px;\n        }\n      }\n    }\n  }\n  .shelf-tip{\n    margin-top: -1.2rem;\n    margin-left: -15px;\n    margin-right: -15px;\n