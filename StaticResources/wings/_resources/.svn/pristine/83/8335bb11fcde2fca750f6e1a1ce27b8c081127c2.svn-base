.ma_bg,body {
	background: url(../../img/mobile/chapter_bg.jpg)
}

.txtbox-item h3 {
	border-bottom: solid 1px #ddd;
	color: #333;
	background: #eee
}

.demo .font-change a {
	border: 1px solid #10c1ae
}

.intro .chapter {
	border-right: 1px solid #ccc
}

.intro .chapter i {
	background: #ccc;
	border: 2px solid #eee
}

.intro .chapter i.red {
	background: #ccc
}

.font_list {
	border-top: solid 1px #ccc
}

.font_list a {
	color: #999
}

.alert-c,.intro .chapter,.intro div {
	color: #333
}

.blue {
	color: #10c1ae!important;
	display: inline-block;
	font-style: normal
}

.chapter-mod-list li a {
	color: #10c1ae;
	border: 1px solid #10c1ae
}

.chapter-time span {
	color: #333
}

.chapter-mod-list li a.first {
	border: 1px solid #ccc;
	color: #999;
	background: #ddd
}

.chapter_spread {
	background: #fff
}

.chapter_spread li a span {
	color: #333
}

.chapter_ma span {
	background: #ebebeb
}
.wline{
	border-bottom: 1px solid #10c1ae;
}
.check-book-mod ul li a p{color:#333;}
.check-book-mod h2{color:#333;}